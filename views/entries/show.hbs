





<main role="main" >
  <ul class="entries-list no-bullets no-padding">
    <p>{{company.name}}</p>
    {{#each arr}}
      <li class="entry-item pad-b-4">
        <h3>"{{body}}"</h3>
          <p class="entry-stub">Author: {{author}}</p>
        <span class="entry-date block font-3-4 c-lt-gray">Intervewed {{dateInterv}}</span>
          <span class="entry-date block font-3-4 c-lt-gray">Created: {{date}}</span>
        <ul class="no-bullets no-padding mar-t-2">
  <li class="pipe-separate left">
    <a href="/entries/{{company_id}}/{{id}}/edit" class="c-white">edit</a>
  </li>
  <li class="pipe-separate left">
    <form method="POST" action="/entries/{{company_id}}/{{id}}" class="inline">
      <input type="hidden" name="_method" value="delete">
      <input type="submit" value="delete" class="no-border no-outline no-bg c-white hover-underline">
    </form>
  </li>
</ul> 


      </li>
    {{/each}}
  </ul>
</main>
